{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "biome lint --colors=off ./src ./index.html",
    "format": "biome format ./src ./index.html --write",
    "check": "biome check ./src ./index.html",
    "preview": "vite preview",
    "test:ui:e2e": "playwright test",
    "dev:mock": "VITE_ENABLE_MOCKS=true vite",
    "preview:mock": "VITE_ENABLE_MOCKS=true vite preview"
  },
  "dependencies": {
    "@iconify/icons-mdi": "^1.2.48",
    "@iconify/react": "^6.0.2",
    "daisyui": "^5.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@eslint/js": "^9.39.1",
    "@faker-js/faker": "^10.1.0",
    "@playwright/test": "^1.51.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react-swc": "^4.2.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "lodash-es": "^4.17.21",
    "msw": "^2.12.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.16",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.2",
    "zod": "^4.1.12"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
